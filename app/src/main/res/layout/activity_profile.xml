<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="22dp"
        android:text="back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="22dp"
        android:text="logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/emial"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="31dp"
        android:text="Username"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="49dp"
        android:text="Screening History"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/s"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/sad"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginTop="76dp"
        android:layout_marginBottom="65dp"
        android:text="Stock Criteria 1"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/asdasd"
        app:layout_constraintEnd_toEndOf="@+id/asdasd"
        app:layout_constraintStart_toStartOf="@+id/asdasd"
        app:layout_constraintTop_toTopOf="@+id/textView" />

    <TextView
        android:id="@+id/dd"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginTop="133dp"
        android:layout_marginBottom="132dp"
        android:text="Stock Criteria 3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dwd1"
        app:layout_constraintEnd_toEndOf="@+id/dwqa"
        app:layout_constraintStart_toStartOf="@+id/dwqa"
        app:layout_constraintTop_toTopOf="@+id/s" />

    <TextView
        android:id="@+id/dwd1"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginEnd="29dp"
        android:text="Stock Criteria 5"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/sssss"
        app:layout_constraintTop_toTopOf="@+id/sssss" />

    <Button
        android:id="@+id/s"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="263dp"
        android:layout_marginEnd="77dp"
        android:onClick="goHistory"
        android:text="View"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ssss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:onClick="goHistory"
        android:text="View"
        app:layout_constraintStart_toStartOf="@+id/sss"
        app:layout_constraintTop_toBottomOf="@+id/sss" />

    <TextView
        android:id="@+id/asdasd"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="20dp"
        android:text="Stock Criteria 2"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/dd"
        app:layout_constraintEnd_toEndOf="@+id/dd"
        app:layout_constraintStart_toStartOf="@+id/dd"
        app:layout_constraintTop_toBottomOf="@+id/s" />

    <Button
        android:id="@+id/ss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:onClick="goHistory"
        android:text="View"
        app:layout_constraintStart_toStartOf="@+id/s"
        app:layout_constraintTop_toBottomOf="@+id/s" />

    <Button
        android:id="@+id/sss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:onClick="goHistory"
        android:text="View"
        app:layout_constraintStart_toStartOf="@+id/ss"
        app:layout_constraintTop_toBottomOf="@+id/ss" />

    <Button
        android:id="@+id/sssss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:onClick="goHistory"
        android:text="View"
        app:layout_constraintStart_toStartOf="@+id/ssss"
        app:layout_constraintTop_toBottomOf="@+id/ssss" />

    <TextView
        android:id="@+id/dwqa"
        android:layout_width="0dp"
        android:layout_height="53dp"
        android:layout_marginTop="66dp"
        android:layout_marginBottom="66dp"
        android:text="Stock Criteria 4"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/dwd1"
        app:layout_constraintEnd_toEndOf="@+id/dwd1"
        app:layout_constraintStart_toStartOf="@+id/dwd1"
        app:layout_constraintTop_toTopOf="@+id/dd" />
</androidx.constraintlayout.widget.ConstraintLayout>